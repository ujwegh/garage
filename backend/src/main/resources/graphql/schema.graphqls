schema {
    query: Query
}

type Query {
    bookById(id: ID): Book
    allBooks: [Book]
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type Gender{
    name: String
}

type User {
    id: ID
    createdDate: String
    updatedDate: String
    lastLoginDate: String
    email: String
    firstName: String
    lastName: String
    gender: String
    phone: String
}